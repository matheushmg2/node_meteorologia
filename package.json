{
  "name": "node_meteorologia",
  "version": "1.0.0",
  "description": "Estudo sobre Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "start": "yarn build && node dist/src/index.js",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "test:unit_auto": "jest --watch",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheushmg2/node_meteorologia.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "matheushmg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matheushmg2/node_meteorologia/issues"
  },
  "homepage": "https://github.com/matheushmg2/node_meteorologia#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "^18.11.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/config": "^3.3.0",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "mongoose": "^6.7.1"
  }
}
